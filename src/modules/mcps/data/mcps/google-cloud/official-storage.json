      "args": ["-y", "@google-cloud/storage-mcp"],
      "env": {}
    }
  },
  "metadata": {
    "homepage": "https://cloud.google.com/storage/docs",
    "repository": "https://github.com/googleapis/gcloud-mcp/tree/main/packages/storage-mcp",
    "addedAt": "2025-12-18",
    "lastUpdated": "2025-12-18"
