{
  "$schema": "../_schema.json",
  "id": "supabase",
  "name": "Supabase",
  "description": "Execute SQL queries, manage database schema, and interact with Supabase projects using natural language",
  "category": "database",
  "icon": "database",
  "logoUrl": "https://cdn.brandfetch.io/idsSceG8fK/w/436/h/449/theme/dark/symbol.png?c=1bxid64Mup7aczewSAYMX&t=1668081497517",
  "source": "official",
  "maintainer": {
    "name": "Supabase",
    "url": "https://supabase.com",
    "github": "supabase"
  },
  "status": "active",
  "version": "1.0.0",
  "transport": "streamable-http",
  "inputs": {
    "SUPABASE_ACCESS_TOKEN": {
      "label": "Supabase Access Token",
      "required": true,
      "type": "password",
      "description": "Personal access token from supabase.com/dashboard/account/tokens"
    },
    "SUPABASE_PROJECT_REF": {
      "label": "Project Reference",
      "required": false,
      "type": "string",
      "description": "Scope access to a specific project (optional, found in project settings URL)"
    }
  },
  "configuration": {
    "template": {
      "type": "streamable-http",
      "url": "https://mcp.supabase.com/mcp?project_ref=${SUPABASE_PROJECT_REF}",
      "headers": {
        "Authorization": "Bearer ${SUPABASE_ACCESS_TOKEN}"
      }
    }
  },
  "metadata": {
    "homepage": "https://supabase.com/docs/guides/getting-started/mcp",
    "addedAt": "2025-01-01",
    "lastUpdated": "2025-12-14"
  }
}